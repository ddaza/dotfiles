{
  "AllAutocomplete.updateOnlyOnSave": true,
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[toml]": {
    "editor.defaultFormatter": "tamasfe.even-better-toml"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "clock.alignment": "Right",
  "clock.format": "hh:MM TT",
  "clock.iconName": "watch",
  "clock.updateInterval": 5000,
  "ctagsSupport.ctagsFilename": "tags",
  "debug.allowBreakpointsEverywhere": false,
  "debug.internalConsoleOptions": "neverOpen",
  "debug.javascript.codelens.npmScripts": "never",
  "debug.onTaskErrors": "abort",
  "debug.openDebug": "neverOpen",
  "debug.showBreakpointsInOverviewRuler": false,
  "debug.showInStatusBar": "never",
  "debug.showInlineBreakpointCandidates": false,
  "diffEditor.ignoreTrimWhitespace": true,
  "editor.autoClosingDelete": "never",
  "editor.bracketPairColorization.enabled": true,
  "editor.fontFamily": "Dank Mono, Cartograph CF, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 18,
  "editor.formatOnSave": true,
  "editor.glyphMargin": false,
  "editor.letterSpacing": -0.5,
  "editor.lineHeight": 1,
  "editor.lineNumbers": "relative",
  "editor.minimap.enabled": true,
  "editor.minimap.maxColumn": 35,
  "editor.minimap.renderCharacters": false,
  "editor.quickSuggestions": {
    "comments": true,
    "other": true,
    "strings": true
  },
  "editor.renderLineHighlight": "all",
  "editor.scrollbar.verticalScrollbarSize": 5,
  "editor.wordBasedSuggestions": "allDocuments",
  "editor.wordWrap": "on",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "markdown": "markdown"
  },
  "emmet.showSuggestionsAsSnippets": true,
  "eslint.options": {},
  "eslint.probe": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html",
    "vue",
    "markdown",
    "json"
  ],
  "explorer.autoReveal": false,
  "extensions.autoUpdate": "onlyEnabledExtensions",
  "files.autoSave": "onFocusChange",
  "files.enableTrash": false,
  "go.lintFlags": ["--fast"],
  "go.lintTool": "golangci-lint",
  "go.testExplorer.enable": true,
  "go.toolsManagement.autoUpdate": true,
  "go.useCodeSnippetsOnFunctionSuggest": true,
  "material-icon-theme.folders.color": "#ccd1f9",
  "playwright.env": {},
  "playwright.reuseBrowser": true,
  "playwright.showTrace": false,
  "prettier.useEditorConfig": false,
  "randomThemeSwitcher.switchMode": "manual",
  "redhat.telemetry.enabled": false,
  "shellcheck.executablePath": "/opt/homebrew/bin/shellcheck",
  "terminal.integrated.macOptionIsMeta": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vim.argumentObjectClosingDelimiters": [")", "]", "}"],
  "vim.argumentObjectOpeningDelimiters": ["(", "[", "{"],
  "vim.easymotion": true,
  "vim.easymotionKeys": "hklyuiopnmqwertzxcvbasdgjf",
  "vim.easymotionMarkerBackgroundColor": "#BCB121",
  "vim.easymotionMarkerFontWeight": "bold",
  "vim.enableNeovim": true,
  "vim.foldfix": true,
  "vim.hlsearch": true,
  "vim.neovimPath": "/usr/local/bin/nvim",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "t", "r"],
      "commands": [":tabmove+"]
    },
    {
      "before": ["<leader>", "t", "l"],
      "commands": [":tabmove-"]
    },
    {
      "before": ["<leader>", "e", "e"],
      "commands": [":e!"]
    },
    {
      "before": ["<leader>", "f", "r"],
      "commands": ["copyRelativeFilePath"]
    },
    {
      "before": ["<leader>", "p"],
      "commands": ["editor.action.formatDocument"]
    },
    {
      "after": ["<leader>", "<leader>", "2", "s"],
      "before": ["<leader>", "z"]
    },
    {
      "before": ["v", "a", "f"],
      "commands": ["editor.action.selectAll"]
    },
    {
      "before": ["<C-w>", "L"],
      "commands": ["workbench.action.moveEditorToRightGroup"]
    },
    {
      "before": ["<C-w>", "H"],
      "commands": ["workbench.action.moveEditorToLeftGroup"]
    }
  ],
  "vim.smartRelativeLine": true,
  "vim.useSystemClipboard": true,
  "vim.visualstar": true,
  "window.confirmBeforeClose": "keyboardOnly",
  "window.customTitleBarVisibility": "never",
  "window.nativeTabs": true,
  "window.zoomLevel": 1,
  "workbench.activityBar.location": "hidden",
  "workbench.colorTheme": "Dracula Theme",
  "workbench.editor.enablePreview": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.editor.tabSizing": "shrink",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.startupEditor": "newUntitledFile",
  "writeTimestamp.bCustomFormat": "[y-mm-dd @ h:MM:SS AA] ",
  "zenMode.hideActivityBar": false
}
