{
  "editor.fontFamily": "Dank Mono, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "window.zoomLevel": 0,
  "vim.useSystemClipboard": true,
  "vim.hlsearch": true,
  "vim.easymotion": true,
  "vim.foldfix": true,
  "files.autoSave": "onFocusChange",
  "vim.visualstar": true,
  "jest.enableSnapshotPreviews": false,
  "jest.enableSnapshotUpdateMessages": false,
  "jest.runAllTestsFirst": false,
  "zenMode.hideActivityBar": false,
  "editor.fontWeight": "600",
  "jest.autoEnable": false,
  "workbench.activityBar.visible": false,
  "workbench.startupEditor": "newUntitledFile",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.lineHeight": 15,
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "editor.wordWrap": "on",
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "diffEditor.ignoreTrimWhitespace": true,
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "eslint.quiet": true,
  "files.enableTrash": false,
  "vim.enableNeovim": true,
  "vim.neovimPath": "/usr/local/bin/nvim",
  "explorer.autoReveal": false,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "t", "r"],
      "commands": [":tabmove+"]
    },
    {
      "before": ["<leader>", "t", "l"],
      "commands": [":tabmove-"]
    },
    {
      "before": ["<leader>", "e", "e"],
      "commands": [":e!"]
    },
    {
      "before": ["<leader>", "f", "r"],
      "commands": ["copyRelativeFilePath"]
    },
    {
      "before": ["<leader>", "p"],
      "commands": ["editor.action.formatDocument"]
    },
    {
      "before": ["<leader>", "z"],
      "after": [
        "<leader>",
        "<leader>",
        "2",
        "s"
      ]
    }
  ],
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "AllAutocomplete.cycleOpenDocumentsOnLaunch": true,
  "AllAutocomplete.updateOnlyOnSave": true,
  "clock.alignment": "Right",
  "clock.format": "hh:MM TT",
  "clock.iconName": "watch",
  "clock.updateInterval": 5000,
  "editor.fontSize": 14,
  "editor.minimap.enabled": true,
  "editor.minimap.maxColumn": 60,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.size": "fill",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vim.easymotionKeys": "hklyuiopnmqwertzxcvbasdgjf",
  "vim.easymotionMarkerBackgroundColor": "yellow",
  "vim.easymotionMarkerFontFamily": "Hack",
  "vim.easymotionMarkerFontSize": "15",
  "vim.easymotionMarkerFontWeight": "bold",
  "vim.easymotionMarkerForegroundColorOneChar": "black",
  "vim.easymotionMarkerForegroundColorTwoChar": "black",
  "vim.easymotionMarkerHeight": 16,
  "yaclock.showAmPm": true,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "material-icon-theme.folders.color": "#ccd1f9",
  "workbench.fontAliasing": "antialiased",
  "editor.fontWeight": "600",
  "workbench.sideBar.location": "left",
  "workbench.iconTheme": "material-icon-theme",
}
