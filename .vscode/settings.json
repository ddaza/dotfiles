{
  "AllAutocomplete.cycleOpenDocumentsOnLaunch": true,
  "AllAutocomplete.updateOnlyOnSave": true,
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "clock.alignment": "Right",
  "clock.format": "hh:MM TT",
  "clock.iconName": "watch",
  "clock.updateInterval": 5000,
  "debug.allowBreakpointsEverywhere": false,
  "debug.internalConsoleOptions": "neverOpen",
  "debug.javascript.codelens.npmScripts": "never",
  "debug.javascript.usePreview": false,
  "debug.onTaskErrors": "abort",
  "debug.openDebug": "neverOpen",
  "debug.showBreakpointsInOverviewRuler": false,
  "debug.showInStatusBar": "never",
  "debug.showInlineBreakpointCandidates": false,
  "diffEditor.ignoreTrimWhitespace": true,
  "editor.autoClosingDelete": "never",
  "editor.bracketPairColorization.enabled": true,
  "editor.fontFamily": "Dank Mono, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 16,
  "editor.formatOnSave": true,
  "editor.glyphMargin": false,
  "editor.lineHeight": 1.2,
  "editor.lineNumbers": "relative",
  "editor.minimap.enabled": true,
  "editor.minimap.maxColumn": 35,
  "editor.minimap.renderCharacters": false,
  "editor.quickSuggestions": {
    "comments": true,
    "other": true,
    "strings": true
  },
  "editor.renderLineHighlight": "all",
  "editor.wordWrap": "on",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "emmet.showSuggestionsAsSnippets": true,
  "eslint.alwaysShowStatus": true,
  "eslint.options": {},
  "eslint.probe": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html",
    "vue",
    "markdown",
    "json"
  ],
  "explorer.autoReveal": false,
  "extensions.autoUpdate": "onlyEnabledExtensions",
  "files.autoSave": "onFocusChange",
  "files.enableTrash": false,
  "material-icon-theme.folders.color": "#ccd1f9",
  "prettier.prettierPath": "prettier",
  "prettier.requireConfig": true,
  "prettier.singleQuote": true,
  "prettier.useEditorConfig": false,
  "redhat.telemetry.enabled": false,
  "stylelint.packageManager": "yarn",
  "stylelint.validate": [
    "css",
    "less",
    "postcss",
    "typescript"
  ],
  "terminal.integrated.macOptionIsMeta": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vim.argumentObjectClosingDelimiters": [
    ")",
    "]",
    "}"
  ],
  "vim.argumentObjectOpeningDelimiters": [
    "(",
    "[",
    "{"
  ],
  "vim.easymotion": true,
  "vim.easymotionKeys": "hklyuiopnmqwertzxcvbasdgjf",
  "vim.easymotionMarkerFontWeight": "bold",
  "vim.enableNeovim": true,
  "vim.foldfix": true,
  "vim.hlsearch": true,
  "vim.neovimPath": "/usr/local/bin/nvim",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<leader>",
        "t",
        "r"
      ],
      "commands": [
        ":tabmove+"
      ]
    },
    {
      "before": [
        "<leader>",
        "t",
        "l"
      ],
      "commands": [
        ":tabmove-"
      ]
    },
    {
      "before": [
        "<leader>",
        "e",
        "e"
      ],
      "commands": [
        ":e!"
      ]
    },
    {
      "before": [
        "<leader>",
        "f",
        "r"
      ],
      "commands": [
        "copyRelativeFilePath"
      ]
    },
    {
      "before": [
        "<leader>",
        "p"
      ],
      "commands": [
        "editor.action.formatDocument"
      ]
    },
    {
      "after": [
        "<leader>",
        "<leader>",
        "2",
        "s"
      ],
      "before": [
        "<leader>",
        "z"
      ]
    },
    {
      "before": [
        "v",
        "a",
        "f"
      ],
      "commands": [
        "editor.action.selectAll"
      ]
    }
  ],
  "vim.useSystemClipboard": true,
  "vim.visualstar": true,
  "window.confirmBeforeClose": "keyboardOnly",
  "window.nativeTabs": true,
  "window.zoomLevel": 1,
  "workbench.activityBar.visible": false,
  "workbench.colorTheme": "Dracula",
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.fontAliasing": "auto",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.sideBar.location": "left",
  "workbench.startupEditor": "newUntitledFile",
  "zenMode.hideActivityBar": false
}
